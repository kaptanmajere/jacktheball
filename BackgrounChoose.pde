    br=255;
    bgr=148;
    bb=148;
    if (b.find(2)==true) {  //Salmon
      br=255;
      bgr=191;
      bb=173;
      if (b.find(3)==true) {  //Orange
        br=255;
        bgr=188;
        bb=130;
        if (b.find(4)==true) {  //Yellow
          br=255;
          bgr=248;
          bb=168;
          if (b.find(5)==true) {  //Lgreen
            br=227;
            bgr=255;
            bb=171;
            if (b.find(6)==true) {  //Aquamarine
              br=154;
              bgr=252;
              bb=215;
              if (b.find(7)==true) {  //Blue
                br=154;
                bgr=223;
